\n            this.props.handleAddToWatchlist(movie)\n        }\n        else{\n            this.props.handleRemoveFromWatchlist(movie)\n        }\n    }\n\n    renderReviews = () => {\n        return this.props.reviews.map(review => {\n            if(review.movie_id === this.props.movie.id){\n                return (\n                    <Review review={review} user={this.props.user}/>\n                )\n            }\n        })\n    }\n\n    render(){\n        return (\n            <Container>\n                    <h1>{this.props.movie.title} ({this.props.movie.year})</h1>\n                    <Button onClick={() => this.handleWatchlist(this.props.movie)}>Add to Watchlist</Button><br/><br/>\n                    <img src={this.props.movie.poster}/><br/><br/>\n                        <h5>Director: {this.props.movie.director}</h5>\n                        <h5>Writer: {this.props.movie.writer}</h5>\n                        <h5>Genres: {this.props.movie.genres}</h5>\n                        <h5>Starring: {this.props.movie.actors}</h5>\n                        <h5>Language: {this.props.movie.language}</h5>\n                        <h5>Rated: {this.props.movie.rated}</h5>\n                        <h5>Plot: {this.props.movie.plot}</h5>\n                    <br/>\n                    <Form>\n                        <h3> Leave a Review</h3>\n                        <Form.Group>\n                            <Form.Label>Rating</Form.Label>\n                            <Form.Control as=\"select\" type=\"rating\" name=\"rating\" value={this.state.rating} onChange={this.handleChange} required >\n                                <option>1 star</option>\n                                <option>2 stars</option>\n                                <option>3 stars</option>\n                                <option>4 stars</option>\n                                <option>5 stars</option>\n                            </Form.Control>    \n                            <Form.Label>Review</Form.Label>\n                            <Form.Control as=\"textarea\" name=\"text\" rows={5} value={this.state.text} onChange={this.handleChange} required/>\n                            <Button onClick={this.handleSubmit}>Submit Review</Button>\n                        </Form.Group>\n                    </Form>\n                    <h3>User Reviews</h3>\n                    {this.renderReviews()}\n            </Container>\n        );\n    }\n}\n\nexport default Movie;\n","/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Review.js",[],{"ruleId":"72","severity":1,"message":"73","line":8,"column":10,"nodeType":"74","messageId":"75","endLine":8,"endColumn":18},{"ruleId":"72","severity":1,"message":"76","line":9,"column":10,"nodeType":"74","messageId":"75","endLine":9,"endColumn":21},{"ruleId":"72","severity":1,"message":"77","line":10,"column":8,"nodeType":"74","messageId":"75","endLine":10,"endColumn":15},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":91,"column":59,"nodeType":"84","messageId":"85","endLine":91,"endColumn":74},{"ruleId":"86","severity":1,"message":"87","line":7,"column":41,"nodeType":"88","messageId":"89","endLine":7,"endColumn":43},{"ruleId":"72","severity":1,"message":"90","line":6,"column":8,"nodeType":"74","messageId":"75","endLine":6,"endColumn":11},{"ruleId":"72","severity":1,"message":"91","line":7,"column":8,"nodeType":"74","messageId":"75","endLine":7,"endColumn":11},{"ruleId":"86","severity":1,"message":"87","line":53,"column":46,"nodeType":"88","messageId":"89","endLine":53,"endColumn":48},{"ruleId":"92","severity":1,"message":"93","line":67,"column":21,"nodeType":"94","endLine":67,"endColumn":57},"no-unused-vars","'Provider' is defined but never used.","Identifier","unusedVar","'createStore' is defined but never used.","'reducer' is defined but never used.","no-native-reassign",["95"],"no-negated-in-lhs",["96"],"no-self