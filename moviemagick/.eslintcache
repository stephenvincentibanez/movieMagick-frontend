leLogin.bind(this)\n    this.handleLogout = this.handleLogout.bind(this)\n  }\n\n  checkLoginStatus(){\n    axios.get('http://localhost:3001/logged_in', { withCredentials: true }).then(response => {\n      console.log('logged in?', response.data.user)\n      // debugger\n      if (response.data.logged_in && this.state.loggedInStatus === 'NOT_LOGGED_IN'){\n        this.setState({\n          loggedInStatus: 'LOGGED_IN',\n          user: response.data.user\n        })\n      } else if (!response.data.logged_in && this.state.loggedInStatus === 'LOGGED_IN'){\n        this.setState({\n          loggedInStatus: 'NOT_LOGGED_IN',\n          user: {}\n        })\n      }\n    }).catch(error => {\n      console.log('check login error', error)\n    })\n  }\n\n  componentDidMount(){\n    this.checkLoginStatus()\n    fetch('http://localhost:3001/movies')\n    .then(r => r.json())\n    .then(movies => this.setState({\n      movies\n    }))\n    fetch('http://localhost:3001/reviews')\n    .then(r => r.json())\n    .then(reviews => this.setState({\n      reviews\n    }))\n    // fetch('http://localhost:3001/watchlists')\n    // .then(r => r.json())\n    // .then(watchlists => this.setState({\n    //   watchlists\n    // }))\n  }\n\n  handleLogout(){\n    this.setState({\n      loggedInStatus: 'NOT_LOGGED_IN',\n      user: {}\n    })\n  }\n\n  handleLogin(data) {\n    this.setState({\n      loggedInStatus: 'LOGGED_IN',\n      user: data.user\n    })\n  }\n\n  handleMovieClick = (movie) => {\n    this.setState({\n      selectedMovie: movie\n    })\n  }\n\n  handleAddToWatchlist = (movie) => {\n    axios.post('http://localhost:3001/watchlists', {\n            watchlist: {user_id: this.state.user.id, movie_id: movie.id}\n        },\n        { withCredentials: true }\n        ).then(response => {\n            if (response.data.status === 'created'){\n                // this.props.handleSuccessfulAuth(response.data)\n            }\n        }).catch(error => {\n            console.log('add watchlist error', error)\n        })\n\n    // this.setState({\n    //   watchlist: [...this.state.watchlist, movie]\n    // })\n  }\n\n  handleRemoveFromWatchlist = (movie) => {\n    this.setState({\n      watchlist: [...this.state.watchlist.filter(movie => movie !== movie)]\n    })\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        {this.state.loggedInStatus === \"LOGGED_IN\" ? <NavBar loggedInStatus={this.state.loggedInStatus}/> : null}\n        <BrowserRouter>\n          <Switch>\n            <Route exact path={'/'} render={props => ( \n              <Home {...props} \n                handleLogin={this.handleLogin} \n                handleLogout={this.handleLogout} \n                loggedInStatus={this.state.loggedInStatus} \n                user={this.state.user}/>)} />\n            <Route exact path={'/browse'} render={props => ( \n              <Browse {...props} \n                user={this.state.user} \n                loggedInStatus={this.state.loggedInStatus} \n                movies={this.state.movies} \n                handleMovieClick={this.handleMovieClick}/>)}/>\n            <Route exact path={'/edit_user'} render={props => (\n              <EditUser {...props} \n                user={this.state.user} \n                loggedInStatus={this.state.loggedInStatus} \n                handleLogout={this.handleLogout}/>)}/>\n            <Route exact path={'/myreviews'} render={props => (\n              <MyReviews {...props} \n                user={this.state.user}/>)}/>\n            <Route exact path={'/watchlist'} render={props => (\n              <Watchlist {...props}\n                user={this.state.user}/>)}/>\n            <Route path={`/movies`} render={props => (\n              <Movie {...props} \n                movie={this.state.selectedMovie} \n                user={this.state.user} \n                reviews={this.state.reviews} \n                handleAddToWatchlist={this.handleAddToWatchlist}\n                handleRemoveFromWatchlist={this.handleRemoveFromWatchlist}\n                watchlist={this.state.watchlist}/>)}/>\n          </Switch>\n        </BrowserRouter>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Home.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/auth/Registration.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/auth/Login.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Browse.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/EditUser.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/MyReviews.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Watchlist.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/NavBar.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/reducers/reducer.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Movie.js",["68","69","70","71"],"import React, { Component } from 'react';\nimport Review from './Review'\nimport axios from 'axios'\nimport Button from 'react-bootstrap/Button'\nimport Container from 'react-bootstrap/Container'\nimport Row from 'react-bootstrap/Row'\nimport Col from 'react-bootstrap/Col'\nimport Form from 'react-bootstrap/Form'\n\nclass Movie extends Component {\n\n    state={\n        rating: '',\n        text: ''\n    }\n\n    handleSubmit = (e) => {\n        e.preventDefault()\n        axios.post('http://localhost:3001/reviews', {\n            review: {\n                movie_id: this.props.movie.id,\n                user_id: this.props.user.id,\n                rating: this.state.rating,\n                text: this.state.text\n            }\n        },\n        { withCredentials: true }\n        ).then(response => {\n            if (response.data.status === 'created'){\n                // this.props.handleSuccessfulAuth(response.data)\n            }\n        }).catch(error => {\n            console.log('review creation error', error)\n        })\n    }\n\n    handleChange = e => {\n        this.setState({\n            [e.target.name]: e.target.value\n        })\n    }\n\n    handleWatchlist = (movie) => {\n        if(!this.props.user.watchlists.includes(movie)){\n            this.props.handleAddToWatchlist(movie)\n        }\n        else{\n            this.props.handleRemoveFromWatchlist(movie)\n        }\n    }\n\n    renderReviews = () => {\n        return this.props.reviews.map(review => {\n            if(review.movie_id === this.props.movie.id){\n                return (\n                    <Review review={review} user={this.props.user}/>\n                )\n            }\n        })\n    }\n\n    render(){\n        return (\n            <Container>\n                    <h1>{this.props.movie.title} ({this.props.movie.year})</h1>\n                    {/* {this.props.user.watchlists.includes(this.props.selectedMovie) ? <Button onClick={() => this.handleWatchlist}> Remove from Watchlist </Button> : <Button onClick={() => this.handleWatchlist(this.props.movie)}>Add to Watchlist</Button> } */}\n                    <Button onClick={() => this.handleWatchlist(this.props.movie)}>Add to Watchlist</Button><br/><br/>\n                    <img src={this.props.movie.poster}/><br/><br/>\n                        <h5>Director: {this.props.movie.director}</h5>\n                        <h5>Writer: {this.props.movie.writer}</h5>\n                        <h5>Genres: {this.props.movie.genres}</h5>\n                        <h5>Starring: {this.props.movie.actors}</h5>\n                        <h5>Language: {this.props.movie.language}</h5>\n                        <h5>Rated: {this.props.movie.rated}</h5>\n                        <h5>Plot: {this.props.movie.plot}</h5>\n                    <br/>\n                    <Form>\n                        <h3> Leave a Review</h3>\n                        <Form.Group>\n                            <Form.Label>Rating</Form.Label>\n                            <Form.Control as=\"select\" type=\"rating\" name=\"rating\" value={this.state.rating} onChange={this.handleChange} required >\n                                <option>1 star</option>\n                                <option>2 stars</option>\n                                <option>3 stars</option>\n                                <option>4 stars</option>\n                                <option>5 stars</option>\n                            </Form.Control>    \n                            <Form.Label>Review</Form.Label>\n                            <Form.Control as=\"textarea\" name=\"text\" rows={5} value={this.state.text} onChange={this.handleChange} required/>\n                            <Button onClick={this.handleSubmit}>Submit Review</Button>\n                        </Form.Group>\n                    </Form>\n                    <h3>User Reviews</h3>\n                    {this.renderReviews()}\n            </Container>\n        );\n    }\n}\n\nexport default Movie;\n","/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Review.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/ReviewDetails.js",["72"],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":109,"column":59,"nodeType":"79","messageId":"80","endLine":109,"endColumn":74},{"ruleId":"81","severity":1,"message":"82","line":6,"column":8,"nodeType":"83","messageId":"84","endLine":6,"endColumn":11},{"rule