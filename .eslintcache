ron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Browse.js",["70"],"import React, {Component} from 'react';\nimport MovieCard from './MovieCard'\nimport Container from 'react-bootstrap/Container'\nimport Row from 'react-bootstrap/Row'\n\n\nclass Browse extends Component {\n    \n    state={\n        search: ''\n    }\n\n    handleSubmit = (e) => {\n        e.preventDefault()\n        const query = e.target.search.value.split(\" \").join(\"+\")\n        fetch(`http://localhost:3001/search?search=${query}`)\n        .then(r => r.json())\n        .then(movies => this.props.handleSearch(movies))\n        .then(\n            this.setState({\n                search: \"\"\n            })\n        )\n    }\n\n    handleChange = e => {\n        this.setState({\n            search: e.target.value\n        })\n    }\n\n    convertKeysToLowercase(data){ \n        for(var i = 0; i < data.length; i++){ \n         for (var key in data[i]) {\n          if(key.toLowerCase() !== key){\n           data[i][key.toLowerCase()] = data[i][key];\n           delete data[i][key];\n          }\n         }\n        }\n       }\n\n    renderCards = () => {\n        if(this.props.searchMovies.length > 0){\n            const searchMovies = this.convertKeysToLowercase(this.props.searchMovies)\n            return this.props.searchMovies.map(movie => {\n                return(\n                    <MovieCard \n                        movie={movie} \n                        handleMovieClick={this.props.handleMovieClick}\n                        user={this.props.user}\n                        handleAddToWatchlist={this.props.handleAddToWatchlist}\n                        handleRemoveFromWatchlist={this.props.handleRemoveFromWatchlist} \n                        />\n                )\n            })\n        }\n        else{\n            return this.props.movies.map(movie => {\n                return(\n                    <MovieCard \n                        movie={movie} \n                        handleMovieClick={this.props.handleMovieClick}\n                        user={this.props.user}\n                        handleAddToWatchlist={this.props.handleAddToWatchlist}\n                        handleRemoveFromWatchlist={this.props.handleRemoveFromWatchlist} \n                        />\n                )\n            })\n        }\n    }\n\n    render(){\n        return (\n            <div>\n                <div className=\"welcome\">\n                    <h1> Browse </h1>\n                    <h5> Welcome {this.props.user.username}!</h5>\n                </div>\n                <form className=\"search\" onSubmit={(e, value) => this.handleSubmit(e, value)}>\n                    <input className=\"search\" type=\"text\" name=\"search\" placeholder=\"Search for a movie!\" value={this.state.search} onChange={(e,value) => this.handleChange(e,value)}></input>\n                </form>\n                <Container className=\"moviesContainer\">\n                    <Row className=\"justify-content-md-center\">\n                        {this.renderCards()}\n                    </Row>\n                </Container>\n            </div>\n        );\n    }\n}\n\nexport default Browse;\n","/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/EditUser.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/MyReviews.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Watchlist.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/NavBar.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Movie.js",["71","72"],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/moviemagick/src/components/Review.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-