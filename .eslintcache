    state={\n        rating: '1 star',\n        text: ''\n    }\n\n    handleSubmit = (e, review) => {\n        e.preventDefault()\n        if(this.props.movie.users.find(u => u.username === this.props.user.username)){\n            alert(\"You've already reviewed this movie!\")\n            this.setState({\n                text: ''\n            })\n        }\n        else{\n            axios.post('http://localhost:3001//reviews', {\n                review: {\n                    movie_id: this.props.movie.id,\n                    user_id: this.props.user.id,\n                    rating: this.state.rating,\n                    text: this.state.text\n                }\n            },\n            { withCredentials: true }\n            ).then(response => {\n                if (response.status === 201){\n                    this.setState({\n                        rating: '',\n                        text: ''\n                    })\n                    alert(\"Your review has been posted!\")\n                    this.props.handleAddReview(review)\n                    }\n                })\n                .catch(error => {\n                console.log('review creation error', error)\n                alert(\"Your review can't be blank!\")\n            })\n        }\n    }\n\n    handleChange = e => {\n        this.setState({\n            [e.target.name]: e.target.value\n        })\n    }\n\n    handleWatchlist = (movie) => {\n        const watchlist = this.props.user.watchlists.find(m => m.movie.title === movie.title)\n        if(!watchlist){\n            axios.post('http://localhost:3001//watchlists', {\n                watchlist: {user_id: this.props.user.id, movie_id: movie.id}\n            },\n            { withCredentials: true }\n            ).then(response => {\n            if (response.status === 201){\n                this.props.handleAddToWatchlist(response.data)\n            }\n            }).catch(error => {\n            console.log('add watchlist error', error)\n        })\n        }\n        else{\n            fetch(`http://localhost:3001//watchlists/${watchlist.id}`, {\n                method: \"DELETE\"\n            }).then(r => r.json())\n            .then(response => {\n                if(response.status === \"destroyed\"){\n                    this.props.handleRemoveFromWatchlist(movie)\n                }\n            })\n        }\n    }\n\n    renderReviews = () => {\n        return this.props.reviews.map(review => {\n            if(review.movie.id === this.props.movie.id){\n                return (\n                    <Review review={review} user={this.props.user}/>\n                )\n            }\n        })\n    }\n\n    render(){\n        return (\n            <div>\n                <h1>{this.props.movie.title} ({this.props.movie.year})</h1>\n            <Container>\n                {/* <br/> */}\n                {this.props.user.watchlists.find(movie => movie.movie.title === this.props.movie.title) ? \n                <Button variant=\"outline-danger\" onClick={() => this.handleWatchlist(this.props.movie)}>Remove from Watchlist</Button> : \n                <Button variant=\"outline-primary\" onClick={() => this.handleWatchlist(this.props.movie)}>Add to Watchlist</Button> }\n                <br/><br/>\n                <Row>\n                    <Col className=\"left\">\n                        <br/>\n                        <h5>Director: {this.props.movie.director}</h5>\n                        <h5>Writer: {this.props.movie.writer}</h5>\n                        <h5>Starring: {this.props.movie.actors}</h5>\n                        <h5>Genres: {this.props.movie.genres}</h5>\n                        <h5>Rated: {this.props.movie.rated}</h5>\n                        <h5>Released: {this.props.movie.released}</h5>\n                        <h5>Runtime: {this.props.movie.runtime}</h5>\n                        <h5>IMDB Rating: {this.props.movie.ratings}</h5>\n                        <h5>Awards: {this.props.movie.awards}</h5>\n                        <h5>Language: {this.props.movie.language}</h5>\n                        <h5>Production Company: {this.props.movie.production}</h5> \n                        <h5>Plot: {this.props.movie.plot}</h5>\n\n                    </Col>\n                    <Col>\n                        <img src={this.props.movie.poster}/><br/><br/>\n                    </Col>\n                </Row>\n                    <br/>\n                <Form className=\"reviewForm\">\n                    <h3> Leave a Review</h3>\n                    <Form.Group>\n                        <h5>Rating</h5>\n                        <Form.Control as=\"select\" type=\"rating\" name=\"rating\" value={this.state.rating} onChange={this.handleChange} >\n                            <option>1 star</option>\n                            <option>2 stars</option>\n                            <option>3 stars</option>\n                            <option>4 stars</option>\n                            <option>5 stars</option>\n                        </Form.Control><br/>\n                        <h5>Review</h5>\n                        <Form.Control as=\"textarea\" name=\"text\" rows={5} value={this.state.text} onChange={this.handleChange} required/>\n                        <Button variant=\"outline-primary\" onClick={this.handleSubmit}>Submit Review</Button>\n                    </Form.Group>\n                </Form>\n                {/* {this.props.movie.reviews.length > 0 ? <h3>User Reviews</h3> : null} */}\n            </Container>\n            <Container>\n                <Row className=\"justify-content-md-center\">\n                    {this.renderReviews()}\n                </Row>\n            </Container>\n            </div>\n        );\n    }\n}\n\nexport default Movie;\n","/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/Browse.js",["73"],"import React, {Component} from 'react';\nimport MovieCard from './MovieCard'\nimport Container from 'react-bootstrap/Container'\nimport Row from 'react-bootstrap/Row'\n\n\nclass Browse extends Component {\n    \n    state={\n        search: ''\n    }\n\n    handleSubmit = (e) => {\n        e.preventDefault()\n        const query = e.target.search.value.split(\" \").join(\"+\")\n        fetch(`http://localhost:3001//search?search=${query}`)\n        .then(r => r.json())\n        .then(movies => this.props.handleSearch(movies))\n        .then(\n            this.setState({\n                search: \"\"\n            })\n        )\n    }\n\n    handleChange = e => {\n        this.setState({\n            search: e.target.value\n        })\n    }\n\n    convertKeysToLowercase(data){ \n        for(var i = 0; i < data.length; i++){ \n         for (var key in data[i]) {\n          if(key.toLowerCase() !== key){\n           data[i][key.toLowerCase()] = data[i][key];\n           delete data[i][key];\n          }\n         }\n        }\n       }\n\n    renderCards = () => {\n        if(this.props.searchMovies.length > 0){\n            const searchMovies = this.convertKeysToLowercase(this.props.searchMovies)\n            return this.props.searchMovies.map(movie => {\n                return(\n                    <MovieCard \n                        movie={movie} \n                        handleMovieClick={this.props.handleMovieClick}\n                        user={this.props.user}\n                        handleAddToWatchlist={this.props.handleAddToWatchlist}\n                        handleRemoveFromWatchlist={this.props.handleRemoveFromWatchlist} \n                        />\n                )\n            })\n        }\n        else{\n            return this.props.movies.map(movie => {\n                return(\n                    <MovieCard \n                        movie={movie} \n                        handleMovieClick={this.props.handleMovieClick}\n                        user={this.props.user}\n                        handleAddToWatchlist={this.props.handleAddToWatchlist}\n                        handleRemoveFromWatchlist={this.props.handleRemoveFromWatchlist} \n                        />\n                )\n            })\n        }\n    }\n\n    render(){\n        return (\n            <div>\n                <div className=\"welcome\">\n                    <h1> Browse </h1>\n                    <h5> Welcome {this.props.user.username}!</h5>\n                </div>\n                <form className=\"search\" onSubmit={(e, value) => this.handleSubmit(e, value)}>\n                    <input className=\"search\" type=\"text\" name=\"search\" placeholder=\"Search for a movie!\" value={this.state.search} onChange={(e,value) => this.handleChange(e,value)}></input>\n                </form>\n                <Container className=\"moviesContainer\">\n                    <Row className=\"justify-content-md-center\">\n                        {this.renderCards()}\n                    </Row>\n                </Container>\n            </div>\n        );\n    }\n}\n\nexport default Browse;\n","/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/Watchlist.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/NavBar.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/NotFound.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/ReviewDetails.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/Review.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/MovieCard.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/auth/Login.js",[],"/Users/stephenvincentibanez/Flatiron/code/movieMagick/movieMagick-frontend/src/components/auth/Registration.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,